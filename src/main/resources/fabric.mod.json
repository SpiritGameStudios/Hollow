{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "Hollow",
  "description": "A mod about hollow logs and hollow promises.",
  "authors": [
    "CallMeEcho",
    "WorldWidePixel"
  ],
  "contact": {
    "repo": "https://github.com/SpiritGameStudios/Hollow",
    "issues": "https://github.com/SpiritGameStudios/Hollow/issues"
  },
  "license": "MPL-2.0",
  "icon": "assets/hollow/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "dev.spiritstudios.hollow.HollowClient"
    ],
    "main": [
      "dev.spiritstudios.hollow.Hollow"
    ],
    "fabric-datagen": [
      "dev.spiritstudios.hollow.datagen.HollowDatagen"
    ],
    "dynamiclights": [
      "dev.spiritstudios.hollow.lambdynamiclights.HollowDynamicLights"
    ],
    "mixinsquared": [
      "dev.spiritstudios.hollow.SpecterMixinCanceller"
    ]
  },
  "mixins": [
    "hollow.mixins.json",
    {
      "config": "hollow.client.mixins.json",
      "environment": "client"
    }
  ],
  "accessWidener": "hollow.accesswidener",
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "~${minecraft_version}-",
    "fabric-api": "*",
    "java": ">=21",
    "specter-core": "*"
  }
}
